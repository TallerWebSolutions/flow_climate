#!/usr/bin/env bash

if
  [[ -s "/Users/celsomartins/workspace/taller/flow_climate/vendor/bundle/ruby/2.7.0/environment" ]]
then
  source "/Users/celsomartins/workspace/taller/flow_climate/vendor/bundle/ruby/2.7.0/environment"
  exec ruby-rewrite "$@"
else
  echo "ERROR: Missing RVM environment file: '/Users/celsomartins/workspace/taller/flow_climate/vendor/bundle/ruby/2.7.0/environment'" >&2
  exit 1
fi

<div class="col-xs-6">
  <div id="projects-count-column"
       class="flow-chart"
       data-title='<%= t('companies.charts.active_projects.title', project_name: @company.name) %>'
       data-xcategories='<%= @strategic_report_data.array_of_months %>'
       data-xtitle='<%= t('charts.xlabel.months') %>'
       data-ytitle='<%= t('charts.ylabel.projects.text') %>'
       data-ylinetitle='<%= t('charts.ylabel.total_hours.text') %>'
       data-columnname='<%= t('charts.ylabel.projects.text') %>'
       data-seriescolumndata='<%= @strategic_report_data.active_projects_count_data %>'
       data-linename='<%= t('charts.ylabel.total_hours.text') %>'
       data-serieslinedata='<%= @strategic_report_data.total_hours_in_month %>'
       data-decimals='0'>
  </div>
</div>

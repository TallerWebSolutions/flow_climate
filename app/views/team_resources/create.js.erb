var teamResourcesDiv = $('#team-flow_events')

<% if @team_resource.errors.empty? %>
  teamResourcesDiv.html("<%= escape_javascript(render 'team_resources/team_resources_table', company: @company, team_resources: @team_resources) %>");

<% else %>
  teamResourcesDiv.html("<%= escape_javascript(render 'team_resources/new', company: @company, team_resource: @team_resource) %>");

  $('#cancel-form-team-resource').on('click', function() {
      teamResourcesDiv.html("<%= escape_javascript(render 'team_resources/team_resources_table', company: @company, team_resources: @team_resources) %>");
  });
<% end %>

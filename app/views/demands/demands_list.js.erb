bindDemandFilterActions();

var demandsList = $("#demands-list");
demandsList.html("<%= escape_javascript(render 'demands/demands_list', demands: @demands, demands_chart_adapter: @demands_chart_adapter, demands_ids: @demands_ids, grouped_delivered_demands: @grouped_delivered_demands, grouped_customer_demands: @grouped_customer_demands).html_safe %>");

var consolidation = $("#content-consolidation");
consolidation.html("<%= escape_javascript(render 'demands/demands_week_consolidation', demands_count_per_week: @demands_count_per_week).html_safe %>");

$('#container-leadtime-div').html("<%= escape_javascript(render 'demands/demands_leadtime_container', confidence_65_leadtime: @confidence_65_leadtime, confidence_80_leadtime: @confidence_80_leadtime, confidence_95_leadtime: @confidence_95_leadtime).html_safe %>");

$('#nav-item-demands').addClass('active');

demandsList.show();
consolidation.hide();
$('#demands-tab').show();

enableTabs();

var flow_impact_div = $('#flow-impacts-tab');

flow_impact_div.html("<%= escape_javascript(render('flow_impacts/edit', company: @company, project: @project, flow_impact: @flow_impact, demands_for_impact_form: @demands_for_impact_form)) %>");

$('#general-loader').hide();
flow_impact_div.show();

$('#cancel-form-flow-impact').on('click', function() {
    flow_impact_div.html("<%= escape_javascript(render('flow_impacts/flow_impacts_tab', company: @company, project: @project, flow_impacts_list: @flow_impacts)) %>");
});

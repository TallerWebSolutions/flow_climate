<div class="row">
  <div class="col-xs-12">
    <%= link_to t('flow_impacts.new.title'), new_company_project_flow_impact_path(company, project), class: 'btn btn-sm btn-success bottom-spaced-component pull-right', remote: true %>
  </div>
</div>

<% if flow_impacts_list.present? %>
  <%= render 'flow_impacts/flow_impacts_table', company: company, project: project, flow_impacts_list: flow_impacts_list %>
<% else %>
  <%= render 'layouts/no_data', missing_data: FlowImpact.model_name.human.pluralize.downcase %>
<% end %>

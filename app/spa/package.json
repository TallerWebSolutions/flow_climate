{
  "name": "flow-climate-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.5.5",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/data-grid": "^4.0.0-alpha.37",
    "@material-ui/icons": "^4.11.2",
    "@mui/icons-material": "^5.2.5",
    "@mui/lab": "^5.0.0-alpha.74",
    "@mui/material": "^5.2.3",
    "@nivo/bar": "^0.79.1",
    "@nivo/core": "^0.79.0",
    "@nivo/line": "^0.79.1",
    "@nivo/pie": "^0.79.1",
    "@nivo/scatterplot": "^0.79.1",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.37",
    "@types/react-csv": "^1.1.2",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.4",
    "currency.js": "^2.0.4",
    "date-fns": "^2.28.0",
    "graphql": "^16.0.1",
    "i18next-resources-to-backend": "^1.0.0",
    "material-ui-confirm": "^3.0.3",
    "react": "^17.0.2",
    "react-component-export-image": "^1.0.6",
    "react-cookie": "^4.1.1",
    "react-csv": "^2.2.2",
    "react-date-picker": "^8.4.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.28.0",
    "react-i18next": "^11.15.3",
    "react-router-dom": "^6.1.1",
    "react-scripts": "^2.1.3",
    "typescript": "^4.5.2",
    "web-vitals": "^1.1.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "PUBLIC_URL=/spa-build react-scripts build",
    "build:watch": "NODE_ENV=development npm run build; npm run watch build",
    "test": "TZ=UTC react-scripts test",
    "eject": "react-scripts eject",
    "type-check": "tsc --pretty --noEmit",
    "lint": "eslint . --ext ts --ext tsx",
    "format": "prettier --write .",
    "watch": "npm-watch",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "engines": {
    "node": "16.13.2"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "npm run lint",
      "npm run format"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "ignorePatterns": [
      "__tests__"
    ],
    "rules": {
      "no-console": "error",
      "max-len": [
        "error",
        {
          "code": 120
        }
      ],
      "complexity": [
        "error",
        20
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/node-logger": "^6.4.18",
    "@storybook/preset-create-react-app": "^1.2.0",
    "@storybook/react": "^6.1.21",
    "@types/i18next": "^13.0.0",
    "@types/jest": "^27.4.0",
    "lint-staged": "^12.1.2",
    "npm-watch": "^0.11.0",
    "prettier": "^2.5.1",
    "webpack": "^5.68.0"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "ts,tsx,json",
      "quiet": false,
      "runOnChangeOnly": true
    }
  }
}
